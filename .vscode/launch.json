    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: single images",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pipeline.py",
            // "args": ["--architecture", "resnet18", "--from-pretrained"],
            "args": ["--architecture", "convnext_tiny", "--from-pretrained", "--weights", "all"], //, "--input-data-folder", "UP_DOWN_stadiazione_CH_gimp"],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: single images k fold",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/pipeline.py",
            // "args": ["--architecture", "resnet18", "--from-pretrained"],
            "args": ["--architecture", "resnet34", "--from-pretrained", "--k-fold", "5", "--input-data-folder", "UP_DOWN_stadiazione_CH_gimp"],
            "console": "integratedTerminal"
        }
    ]
}