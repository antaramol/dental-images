    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: pipeline",
            "type": "debugpy",
            "request": "launch",
            "module": "pipeline",
            "args": ["--input_data_folder", "UP_DOWN_stadiazione_CH_gimp"],
            "console": "integratedTerminal"
        }
    ]
}